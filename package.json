    "docker:down": "docker-compose down"
  },
  "keywords": ["fuel", "tax", "nodejs", "react", "sequelize"],
  "author": "Lucas",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

